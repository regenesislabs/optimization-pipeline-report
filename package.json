{
  "name": "optimization-pipeline-report",
  "version": "2.0.0",
  "private": true,
  "description": "Asset optimization pipeline report generator for Decentraland",
  "workspaces": [
    "ui"
  ],
  "scripts": {
    "build": "tsc",
    "build:ui": "npm run build --workspace=ui",
    "build:server": "tsc -p tsconfig.server.json",
    "start": "node dist/index.js",
    "server": "node dist/server/index.js",
    "dev": "ts-node src/index.ts",
    "dev:ui": "npm run dev --workspace=ui",
    "dev:server": "ts-node src/server/index.ts",
    "generate": "npm run build && npm start"
  },
  "keywords": ["decentraland", "optimization", "report"],
  "author": "DCL Explorer",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dclexplorer/optimization-pipeline-report.git"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.0.0",
    "@well-known-components/env-config-provider": "^1.2.0",
    "@well-known-components/http-server": "^2.1.0",
    "@well-known-components/interfaces": "^1.4.3",
    "@well-known-components/logger": "^3.1.3",
    "axios": "^1.6.2",
    "dotenv": "^16.3.1",
    "pg": "^8.11.3"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/pg": "^8.10.9",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  }
}